@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="row">

    @if (TempData.Count > 0)
    {
        if (TempData["successs"] != null && !string.IsNullOrWhiteSpace(TempData["successs"].ToString()))
        {
            <div class="alert alert-success fade in">
                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                <i class="fa fa-check alert-icon"></i><strong>Well done!</strong> @TempData["successs"]
            </div>
        }
    }

    <h1>
        @TempData["success"]
    </h1>

</div>