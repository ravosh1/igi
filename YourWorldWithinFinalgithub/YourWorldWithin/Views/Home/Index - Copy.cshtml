        <div class="tnit-popup-outer">

            <!-- Modal Box Start-->
            <div class="modal fade tnit-modal-outer" id="tnit-modal" tabindex="-1" role="dialog" aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="col-md-12">

                            <div class="col-md-1">

                            </div>
                            <div class="col-md-10">
                                <div class="modal-header">
                                    <div class="top">
                                        <h5 class="modal-title"><a href="#">New to site?</a> Create an Account</h5>
                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                            <span aria-hidden="true">&times;</span>
                                        </button>
                                    </div>
                                    <h3>Register</h3>
                                </div>
                                <div class="modal-body">
                                    <!--Modal Form Start-->
                                    @using (Html.BeginForm("UserRegister", "Account", FormMethod.Post, new { enctype = "multipart/form-data", @class = "register" }))
                                    {
                                        if (TempData["success"] != null)
                                        {
                                            <div class="alert alert-success fade in blue-alert" role="alert">
                                                <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
                                                <strong>@TempData["success"]</strong>
                                            </div>
                                        }
                                        else if (TempData["error"] != null)
                                        {
                                            <div class="alert alert-warning fade in blue-alert" role="alert">
                                                <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
                                                <strong>@TempData["error"]</strong>
                                            </div>
                                        }

                                        <div class="inner-holder">
                                            <label>Name</label>
                                            @Html.TextBoxFor(model => model.Name, new { @placeholder = "Name", @required = "required", @class = "form-control input-lg" })

                                        </div>
                                        <div class="inner-holder">
                                            <label>Email</label>
                                            @Html.TextBoxFor(model => model.Email, new { @type = "Email", @placeholder = "Email", @required = "required", @class = "input-lg form-control" })

                                        </div>
                                        <div class="inner-holder">
                                            <label>Phone</label>
                                            @Html.TextBoxFor(model => model.Phone, new { @placeholder = "Phone", @required = "required", @class = "input-lg form-control", @onkeypress = "return isNumber(event,this);" })

                                        </div>
                                        <div class="inner-holder">
                                            <label>Password</label>
                                            @Html.TextBoxFor(model => model.Password, new { @type = "password", @placeholder = "Password", @required = "required", @class = "input-lg form-control", @onchange = "passwordmatch()", @onfocus = "passwordmatch()" })


                                        </div>
                                        <div class="inner-holder">
                                            <label>Confirm Password</label>
                                            @Html.TextBoxFor(model => model.ConPassword, new { @type = "Password", @placeholder = "Confirm Password", @required = "required", @class = "input-lg form-control", @onchange = "passwordmatch()", @onfocus = "passwordmatch()" })
                                            <span style="color:red;font-size:14px;" id="p1"></span>
                                        </div>
                                        <div class="inner-holder text-center">
                                            <div class="col-sm-6">
                                                <input value="Register" class="btn-register" type="submit" style="margin-top:20px;">

                                            </div>
                                            <div class="col-sm-6">
                                                <input value="Close" class="btn-register" data-dismiss="modal" type="button" style="margin-top:20px;background-color:darkseagreen">

                                            </div>

                                        </div>
                                    }<!--Modal Form End-->
                                </div>
                            </div>

                            <div class="col-md-1">
                            </div>
                        </div>

                    </div>
                </div>
            </div><!-- Modal Box End-->
        </div>
        </div><!--Wrapper Content End-->
        <!--JQuery Files Start-->
        <script src="@Url.Content("~/js/jquery-3.1.1.min.js")"></script>
        <script>
            $(document).ready(function () {

                if ('@TempData["msg"]' == "1") {
                    $("#model1").click();

                }

            });
            function passwordmatch() {
                if ($("#Password").val() != $("#ConfirmPassword").val()) {
                    $("#p1").html('Confirm Password Not Match');
                    // $("#btnregister1").prop('disabled', true);
                }
                else {
                    $("#p1").html('');
                    //$("#btnregister1").prop('disabled', false);;
                }
            }

        </script>
        <script type="text/javascript">

            function isNumber(evt, element) {
                var charCode = (evt.which) ? evt.which : event.keyCode
                if (
                    (charCode != 46 || $(element).val().indexOf('.') != 0) &&      // “.” CHECK DOT, AND ONLY ONE.
                    (charCode < 48 || charCode > 57))
                    return false;
                return true;
            }

        </script>
