                            <a href="#" class="tnit-btn-download"><i class="fa fa-download" aria-hidden="true"></i> Download Book</a>
                        </div><!--Widget End-->
                        <!--Widget Start-->
                        <div class="widget widget-tweets">
                            <h3>Follow Me</h3>
                            <div class="row tnit-social-row_v2">
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <!--Socail Box Start-->
                                    <div class="tnit-social-box after-line">
                                        <a href="#"><i class="fa fa-youtube" aria-hidden="true"></i></a>
                                        <div class="tnit-text">
                                            <strong>1,43,000</strong>
                                            <span>Subscribers</span>
                                        </div>
                                    </div><!--Socail Box End-->
