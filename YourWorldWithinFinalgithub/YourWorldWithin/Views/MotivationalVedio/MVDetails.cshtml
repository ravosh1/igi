@model YourWorldWithin.Models.Property
@{
    ViewBag.Title = "MVDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!--Inner Banner Content Start-->
<div class="tnit-inner-banner tnit-banner-image_v2">
    <div class="container">
        <h2>Video Detail</h2>
        <!--BreadCrumb Listed Start-->
        <ul class="breadcrumb tnit-breadcrumb-holder">
            <li class="breadcrumb-item"><a href="index.html">Home</a></li>
            <li class="breadcrumb-item active">Video Detail</li>
        </ul><!--BreadCrumb Listed End-->
    </div>
</div><!--Inner Banner Content End-->
<!--Main Content Start-->
<div class="tnit-main-content">
    <!--Video Section Start-->
    <section class="tnit-video-section tnit-video-section-detail pd-tb70">
        <div class="container">
            <div class="row">
                <div class="col-md-9 col-sm-12 col-xs-12">
                    <!--Video Holder Start-->
                    @*<div class="tnit-video-holder" >*@
                        
                        <video style="width: 100%;" controls src="@Url.Content("~/DataImages/Videos/"+Model.VideoFile)" preload="none"></video>

                    @*</div>*@
                    <!--Video Holder End-->
                    <!--Video Text Start-->
                    <div class="tnit-video-text">
                        <div class="top">
                            <h3>@Model.Title</h3>
                            <ul class="tnit-meta-listed">
                                <li>
                                    <i class="fa fa-user" aria-hidden="true"></i> <a href="#">Eddie Pinero</a>
                                </li>
                                <li>
                                    <i class="fa fa-calendar-minus-o" aria-hidden="true"></i> @Model.creationdate
