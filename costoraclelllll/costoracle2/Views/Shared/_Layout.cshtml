<script>
        $(document).ready(function () {
            $('[data-toggle="popover"]').popover({
                placement: 'right',
                trigger: 'hover'
            });
        });
    </script>

    <script>
        $(document).ready(function () {
            $("input[name$='vehicle']").click(function () {
                var test = $(this).val();

                $("div.desc").hide();
                $("#vehicle" + test).show();
            });
        });
    </script>
    <script>
        $(window).scroll(function () {
            var scroll = $(window).scrollTop();

            if (scroll >= 90) {
                $("header").addClass("sticky");
            } else {
                $("header").removeClass("sticky");
            }
        });
    </script>
</body>
</html>